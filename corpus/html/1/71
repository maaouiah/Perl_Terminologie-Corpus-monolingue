										function selectItem(li) {
											var formn = document.getElementById('form_search_all');
											var vs = li.selectValue;
											if (vs == '')return;
											formn.action = formn.action.substr(0,formn.action.indexOf('recherche/')+10);
											if(document.getElementById("checkhardhide")){
											 	if(document.getElementById("checkhardhide").value=="1"){
		 										 	var newstr = formn.action.substr(0,formn.action.indexOf('recherche/')) ;
		 	 										newstr = newstr + 'comparer-les-prix/recherche/';
		 										 	formn.action = newstr;
		 										}
											}
											if(document.getElementById("checkforumhide")){
											 	if(document.getElementById("checkforumhide").value=="1"){
