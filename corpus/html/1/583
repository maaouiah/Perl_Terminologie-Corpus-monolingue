<script type="text/javascript">
$(document).ready(function () {
var slideWidth = 912;
var nbVisibleBrands = 6;
var slides = $('.slideElem');
var nbSlides = Math.ceil((slides.length) / nbVisibleBrands);
var currentPosition = 0;
// Supprime l'overflow
$('#slidesWrapper').css('overflow', 'hidden');
// Défini la largeur #slideInner = à toutes les largeurs des slides
$('#slidesInner').css('width', slideWidth * nbSlides);
// Boutons de navigation
animSlides(currentPosition);
// Clic sur les boutons
$('.slidecontrol').bind('click', function () {
if ($(this).is('.active')) {
// Nouvelle position
currentPosition = ($(this).attr('id') == 'rightSlide') ? currentPosition + 1 : currentPosition - 1;
if (currentPosition >= 0 && currentPosition < nbSlides) {
// Maj boutons
animSlides(currentPosition);
$('#slidesInner').animate({
'marginLeft': slideWidth * (-currentPosition)
});
}
}
});
function animSlides(position) {
// Cache le bouton gauche si on est sur le premier slide
if (position == 0) { $('#leftSlide').removeClass("active").toggleClass("inactive", true); } else { $('#leftSlide').removeClass("inactive").toggleClass("active", true); }
// Cache le bouton droite si on est sur le dernier .slide
if (position == nbSlides - 1) { $('#rightSlide').removeClass("active").toggleClass("inactive", true); } else { $('#rightSlide').removeClass("inactive").toggleClass("active", true); }
}
});
</script>

