<script type="text/javascript">    
    function pageLoad(sender, args) {
        var modalPopupComponent = $find('ctl00_cphMainContent_ucComparatorAddControl_mpeCompare');
        if (modalPopupComponent == null) { return; }
        modalPopupComponent.add_shown(function () {
            $("html").css("overflow", "hidden");
            $('#ctl00_cphMainContent_ucComparatorAddControl_mpeCompare_backgroundElement').css("width", document.body.offsetWidth + "px");

            // Si popup plus petite que le contenu (en hauteur), on fait scroller la partie centrale
            var iPopupHeight = $(".PopCompare").height(); // max-height à 90% dans le css
            var iTotalHeight = $("#tblParams").height() + $("table.ListingCompare.top").height() + $("table.ListingCompare.footer").height();
            if (iPopupHeight < iTotalHeight) {
                var iOverflowHeight = iPopupHeight - $("table.ListingCompare.top").height() - $("table.ListingCompare.footer").height() - 30; // 30px pour les images des flèches de scroll
                // Si scroll horizontal
                if ($(".PopCompare").width() < $("table.ListingCompare.top").width()) {
                    iOverflowHeight = iOverflowHeight - 20;
                }
                $("div.ListingCompare.overflow").css("height", iOverflowHeight + "px");
            }
            else {
                $("a.btParam").hide();
            }

            //var iCurrentHeight = $("#tblParams").height();
            //var iMaxHeight = $("div.ListingCompare.overflow").height();
            //if (iCurrentHeight < iMaxHeight) {
            //$("div.ListingCompare.overflow").css("height", iCurrentHeight + "px");
            //$("a.btParam").hide();
            //}
        });        
        modalPopupComponent.add_hiding(function() {            
            $("html").css("overflow", "auto");            
        });
    }
</script> 
