<script type="text/javascript">
$(document).ready(function () {
$('#SeoMap').insertAfter('.footercontent').css('height', 'auto').css('overflow', 'auto');
});
// l'index du dernier onglet que l'on a chargé
var last_index_onglet_load = 0;
function pageLoad() {
// mouseenter - pour chaque onglet de l'updatepanel TopProduct
$("#ctl00_cphMainContent_upnlTopProduct li").mouseenter(function () {
// l'index de l'onglet sur lequel est la souris et que l'on veut charger
var current_index_onglet_load = $("#ctl00_cphMainContent_upnlTopProduct li").index(this);
// on vérifie qu'il s'agit d'un nouvel onglet à charger
if (current_index_onglet_load != last_index_onglet_load) {
// UniqueID de hplCat contenu ds l'attribut rel
var uniqueid = $(this).children("a").attr("rel");
__doPostBack(uniqueid, '');
}
last_index_onglet_load = current_index_onglet_load;
});
}
</script>

