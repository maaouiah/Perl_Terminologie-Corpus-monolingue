	function refreshAds(){
        var levels = '';
       
		
		if (document.getElementById('pub_dart_MBR_T_728x90') != null){
	       		document.getElementById('pub_dart_MBR_T_728x90').innerHTML = '<iframe width=\"728\" height=\"90\" frameborder=\"0\" scrolling=\"no\" marginwidth=\"0\" marginheight=\"0\" src=\"/api/doubleclick.php?g=MBR_T_728x90&c=' + levels + '\"></iframe>';
